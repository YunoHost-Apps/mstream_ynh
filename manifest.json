{
    "name": "mStream",
    "id": "mstream",
    "packaging_format": 1,
    "description": {
        "en": "Music streaming server",
        "fr": "Serveur de streaming musical"
    },
    "version": "5.11.4~ynh1",
    "url": "https://mstream.io/",
    "upstream": {
        "license": "AGPL-3.0-only",
        "website": "https://mstream.io/",
        "demo": "https://demo.mstream.io/?",
        "code": "https://github.com/IrosTheBeggar/mStream"
    },
    "license": "AGPL-3.0-only",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/mstream",
                "default": "/mstream"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
