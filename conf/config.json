{
  "port": __PORT__,
  "webAppDirectory": "public",
  "secret": "__KEY__",
  "writeLogs": true,
  "noUpload": false,
  "scanOptions": {
    "skipImg": true,
    "scanInterval": 1.5,
    "pause": 50,
    "saveInterval": 500,
    "bootScanDelay": 15
  },
  "storage": {
    "albumArtDirectory": "__DATA_DIR__/album-art",
    "dbDirectory": "__DATA_DIR__/db",
    "logsDirectory": "__DATA_DIR__/logs"
  },
  "transcode": {
    "enabled": true,
    "ffmpegDirectory": "/usr/bin/ffmpeg",
    "defaultCodec": "opus",
    "defaultBitrate": "128k"
  }
}